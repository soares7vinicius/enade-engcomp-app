<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.pdm.enade_engcomp_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".EnadeECompApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.LoginActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.AccountActivity" />
        <activity android:name=".activities.SimulatedActivity" />
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".activities.CorrectionQuestionActivity" />
        <activity android:name=".activities.RankingActivity"/>
        <activity android:name=".activities.CorrectedSimulationActivity"/>
    </application>

</manifest>